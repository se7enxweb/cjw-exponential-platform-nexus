{
    "browserslist": {
        "defaults": [
            "last 2 versions",
            "Safari >= 10",
            "iOS >= 10",
            "not ie <= 10",
            "> 1%"
        ],
        "evergreen": [
            "last 2 Chrome versions",
            "last 2 ChromeAndroid versions",
            "last 2 Firefox versions",
            "last 2 FirefoxAndroid versions",
            "last 2 Safari versions",
            "last 2 iOS versions",
            "last 2 Edge versions",
            "last 2 Opera versions"
        ]
    },
    "scripts": {
        "dev": "NODE_OPTIONS=--openssl-legacy-provider encore dev",
        "build:dev": "NODE_OPTIONS=--openssl-legacy-provider encore dev",
        "prod": "NODE_OPTIONS=--openssl-legacy-provider encore production",
        "build:prod": "NODE_OPTIONS=--openssl-legacy-provider encore production",
        "watch": "NODE_OPTIONS=--openssl-legacy-provider encore dev --watch",
        "start": "NODE_OPTIONS=--openssl-legacy-provider encore dev-server",
        "server": "NODE_OPTIONS=--openssl-legacy-provider encore dev-server",
        "site:dev": "NODE_OPTIONS=--openssl-legacy-provider encore dev --config-name",
        "site:prod": "NODE_OPTIONS=--openssl-legacy-provider encore production --config-name",
        "site:watch": "NODE_OPTIONS=--openssl-legacy-provider encore dev --watch --config-name",
        "site:server": "NODE_OPTIONS=--openssl-legacy-provider encore dev-server --config-name",
        "ezplatform": "NODE_OPTIONS=--openssl-legacy-provider encore production --config=webpack.config.ezplatform.js",
        "build:parallel": "NODE_OPTIONS=--openssl-legacy-provider parallel -j2 encore ::: dev production",
        "site:parallel": "NODE_OPTIONS=--openssl-legacy-provider echo '--config-name'; | parallel -j2 encore {} ::: dev production"
    },
    "private": true,
    "devDependencies": {
        "@babel/core": "^7.8.3",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/preset-env": "^7.8.3",
        "@babel/preset-react": "^7.8.3",
        "@symfony/webpack-encore": "^0.27.0",
        "autoprefixer": "^9.7.4",
        "babel-loader": "^8.0.0",
        "css-loader": "^2.0.0",
        "eslint": "^5.16.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-plugin-import": "^2.20.0",
        "eslint-plugin-jsx-a11y": "^6.2.1",
        "eslint-plugin-react": "^7.18.0",
        "file-loader": "^6.2.0",
        "parallel": "^1.2.0",
        "path": "^0.12.7",
        "postcss-loader": "^3.0.0",
        "postcss-url": "^10.1.3",
        "sass": "^1.50.0",
        "sass-lint": "^1.12.1",
        "sass-loader": "^10.4.1",
        "style-loader": "^1.0.0",
        "webpack": "^4.0.0"
    },
    "dependencies": {
        "20": "^3.1.9",
        "@babel/polyfill": "^7.8.3",
        "@fortawesome/fontawesome-free": "^5.8.1",
        "@netgen/javascript-cookie-control": "^0.0.6",
        "blueimp-gallery": "^2.36.0",
        "bootstrap": "^4.3.1",
        "bootstrap-datepicker": "^1.9.0",
        "jquery": "^3.4.0",
        "jquery-observe": "^0.2.3",
        "magnific-popup": "^1.1.0",
        "node": "^25.6.1",
        "popper.js": "^1.16.1",
        "swiper": "^4.5.0"
    }
}
