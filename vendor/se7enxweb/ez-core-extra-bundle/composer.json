{
    "name": "se7enxweb/ez-core-extra-bundle",
    "description": "Extra sugar for building eZ Platform websites",
    "require": {
        "se7enxweb/ezpublish-kernel": "~7.5.33",
        "twig/twig": "^2.16.1",
        "symfony/config": "^3.4",
        "symfony/event-dispatcher": "^3.4",
        "symfony/dependency-injection": "^3.4",
        "symfony/security-core": "^3.4",
        "symfony/http-foundation": "^3.4",
        "symfony/http-kernel": "^3.4",
        "symfony/expression-language": "^3.4",
        "symfony/options-resolver": "^3.4"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Jerome Vieilledent",
            "email": "lolautruche@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {"Lolautruche\\EzCoreExtraBundle\\": ""}
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mikey179/vfsstream": "^1.6.3"
    },
    "suggest": {
        "ezsystems/ezplatform-design-engine": "Design fallback mechanism for eZ Platform (previously part of EzCoreExtraBundle)"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
